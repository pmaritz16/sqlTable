{
  "name": "csv-to-sql-loader",
  "version": "1.0.0",
  "description": "Load CSV files into SQL database tables with Svelte GUI",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && electron .",
    "electron": "electron .",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:dev:127": "set OLLAMA_HOST=127.0.0.1 && concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:electron": "vite build && electron-builder"
  },
  "dependencies": {
    "sql.js": "^1.10.3",
    "svelte": "^4.2.7"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "vite": "^5.0.0",
    "electron": "^28.0.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}

